                    </div>
                    <input type="text" class="form-control" name="inputAmount" placeholder="0.00">
                  </div>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputTransDate">Transaction Date</label>
                <div class="col-12 col-sm-8">
                  <input
                    type="date"
                    class="form-control"
                    name="inputTransDate"
                  />
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputContactName">Contact Name</label>
                <div class="col-12 col-sm-8">
                  <input type="text" class="form-control" name="inputContactName" placeholder="Name"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputType">Category</label>
                <div class="col-12 col-sm-8">
                  <select type="" class="form-control" name="inputTransactionCategory" data-show-content="true">
                    <option value="selected disabled">Select from the options</option>
                    <option class="alert-success" value="Produce Sales">Produce Sales</option>
                    <option class="alert-success" value="Livestock Sales">Livestock Sales</option>
