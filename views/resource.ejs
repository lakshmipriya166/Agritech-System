    <!-- Contact Modal Form -->
    <div id="contactModal" class="modal fade" role="dialog">
      <div class="modal-dialog modal-lg" role="content">
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header alert-success">
            <h4 class="modal-title">Add Contact</h4>
            <button type="button" class="close" data-dismiss="modal">&times;</button>
          </div>
          <div class="modal-body">
            <form id="contact-form" action="/contact" method="post">
              <input type="hidden" name="userid" value="<%=userid%>">
              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label"
                  >Full Name</label
                >
                <div class="col-12 col-sm-4">
                  <input type="text" class="form-control" name="inputFirstName" placeholder="First Name"/>
                </div>
                <div class="col-12 col-sm-4">
                  <input type="text" class="form-control" name="inputLastName" placeholder="Last Name"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputDOB">DOB</label>
                <div class="col-12 col-sm-8">
                  <input
                    type="date"
                    class="form-control"
                    id="inputDOB"
                    name="inputDOB"
                  />
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputEmail">Email</label>
                <div class="col-12 col-sm-8">
                  <input type="email" class="form-control" name="inputEmail" placeholder="abc@gmail.com"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputType">Type</label>
                  <div class="col-12 col-sm-8">
                    <select type="" class="form-control" name="inputType" data-show-content="true">
                      <option value="selected disabled">Select a type of contact</option>
                      <option value="Buyer">Buyer</option>
                      <option value="Retail Customer">Retail Customer</option>
                      <option value="Wholesale Customer">Wholesale Customer</option>
                      <option value="Supplier">Supplier</option>
                      <option value="Employee">Employee</option>
                      <option value="Random Contact">Random Contact</option>
                  </select>
                  </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputTelephone">Telephone</label>
                <div class="col-12 col-sm-8">
                  <input type="tel" class="form-control" name="inputTelephone" placeholder="(044)12345678"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputMobile">Mobile</label>
                <div class="col-12 col-sm-8">
                  <input type="tel" class="form-control" name="inputMobile" placeholder="+91 0123456789"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputCompany">Company</label>
                <div class="col-12 col-sm-8">
                  <input type="text" class="form-control" name="inputCompany" placeholder="Company Name"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputAddress">Street Name</label>
                <div class="col-12 col-sm-8">
                  <input type="text" class="form-control" name="inputAddress" placeholder="Delivery Address"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label"
                  >City</label
                >
                <div class="col-12 col-sm-4">
                  <select type="" class="form-control" name="inputCity" data-show-content="true">
                    <option value="selected disabled">City Name</option>
                    <option value="Bengaluru">Bengaluru</option>
                    <option value="Chennai">Chennai</option>
                    <option value="Hyderabad">Hyderabad</option>
                    <option value="Kolkata">Kolkata</option>
                    <option value="Mumbai">Mumbai</option>
                    <option value="New Delhi">New Delhi</option>
                  </select>
                </div>
                <div class="col-12 col-sm-4">
                  <input type="text" class="form-control" name="inputPostalCode" placeholder="600 020"/>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputState">State</label>
                <div class="col-12 col-sm-8">
                  <select type="" class="form-control" name="inputState" data-show-content="true">
                    <option value="selected disabled">State Name</option>
                    <option value="Karnataka">Karnataka</option>
                    <option value="Tamil Nadu">Tamil Nadu</option>
                    <option value="Telangana">Telangana</option>
                    <option value="West Bengal">West Bengal</option>
                    <option value="Maharashtra">Maharashtra</option>
                    <option value="Andhra Pradesh">Andhra Pradesh</option>
                  </select>
                </div>
              </div>

              <div class="form-group row">
                <label class="col-12 col-sm-2 col-form-label" for="inputNotes">Notes</label>
                <div class="col-12 col-sm-8">
                  <textarea class="form-control" name="inputNotes" placeholder="Description, related links, ...."></textarea>
                </div>
              </div>

              <div class="form-group row">
                <div class="col offset-0 offset-sm-2">
                  <button type="button" class="btn btn-secondary btn-sm ml-auto" data-dismiss="modal">
                    Cancel
                  </button>
                  <button type="submit" class="btn btn-primary btn-sm ml-auto">
                    Add
                  </button>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

