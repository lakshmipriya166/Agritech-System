                    <option class="alert-danger" value="Material or Equipment Resale">Material or Equipment Resale</option>
                    <option class="alert-danger" value="Truck or Tractor Expense">Truck or Tractor Expense</option>
                    <option class="alert-danger" value="Fertilizers">Fertilizers</option>
                    <option class="alert-danger" value="Livestock Purchase">Livestock Purchase</option>
                    <option class="alert-danger" value="Irrigation System">Irrigation System</option>
                    <option class="alert-danger" value="Tools Purchase">Tools Purchase</option>
                    <option class="alert-danger" value="Maintenance">Maintenance</option>
                    <option class="alert-danger" value="Employee">Employee</option>
                    <option class="alert-primary" value="Other">Other</option>
                  </select>
                </div>
              </div>

              <div class="form-group row">
